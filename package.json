{
  "name": "solid-launch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "//old:dev": "concurrently --names 'vite,ws' 'bunx --bun vite --host' 'bun run --watch src/server/dev-ws-server.ts'",
    "//old:ws": "bun run --watch src/server/dev-ws-server.ts",
    "//old:dev2": "bun run --watch src/server/server.ts",
    "dev": "nodemon --watch src/server --ext ts --exec 'bun run src/server/server.ts'",
    "build": "tsc && vite build",
    "preview": "NODE_ENV=production bun run src/server/server.ts",
    "preview-static": "vite preview",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:generate": "bunx prisma generate",
    "db:migrate": "prisma migrate dev",
    "outdated": "bunx npm-check-updates --interactive --format group"
  },
  "dependencies": {
    "@felte/solid": "^1.2.13",
    "@felte/validator-zod": "^1.0.17",
    "@hono/node-server": "^1.12.0",
    "@hono/trpc-server": "^0.3.1",
    "@kobalte/core": "^0.13.3",
    "@libsql/kysely-libsql": "^0.3.0",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.16.2",
    "@tanstack/solid-query": "^5.51.2",
    "@trpc/client": "^11.0.0-rc.390",
    "@trpc/server": "^11.0.0-rc.390",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "hono": "^4.5.3",
    "kysely": "^0.27.3",
    "kysely-bun-worker": "^0.6.1",
    "lucia": "^3.2.0",
    "minimatch": "^9.0.5",
    "prisma": "^5.16.2",
    "prisma-kysely": "^1.8.0",
    "solid-js": "^1.8.18",
    "solid-sonner": "npm:@blankeos/solid-sonner",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "trpc-bun-adapter": "^1.1.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.13.1",
    "@iconify-json/line-md": "^1.1.38",
    "@tanstack/solid-query-devtools": "^5.51.2",
    "@types/bun": "^1.1.6",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "selfsigned": "^2.4.1",
    "tailwindcss": "^3.4.5",
    "typescript": "^5.5.4",
    "unplugin-icons": "^0.19.0",
    "vike": "^0.4.177",
    "vike-solid": "^0.6.1",
    "vite": "^5.3.5",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-solid-svg": "^0.8.1"
  }
}
